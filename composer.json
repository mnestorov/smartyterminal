{
    "name": "smartystudio/smartyterminal",
    "description": "SmartyTerminal is a Laravel console/terminal, for the SmartyCMS content management system (CRUD) package.",
    "homepage": "https://smartycms.net",
    "keywords": [
        "terminal",
        "console",
        "web",
        "web artisan"
    ],
    "type": "library",
    "license": "Proprietary",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Smarty Studio MBN Ltd.",
            "homepage": "https://smartystudio.net",
            "email": "office@smartystudio.net"
        },
        {
            "name": "Martin Nestorov",
            "email": "mbnestorov@gmail.com",
            "role": "Lead Developer"
        }
    ],
    "support": {
        "issues": "https://smartycms.net/issues"
    },
    "autoload": {
        "files": [
            "src/Polyfill.php"
        ],
        "psr-4": {
            "SmartyStudio\\SmartyTerminal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartyStudio\\SmartyTerminal\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/config": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/console": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/events": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/filesystem": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/routing": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/view": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "symfony/finder": "^2.7|^3.0|^4.0|^5.0|^6.0",
        "webmozart/glob": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.1|^6.1|^7.0|^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.4",
        "roave/security-advisories": "dev-latest",
        "mikey179/vfsstream": "^1.6",
        "nesbot/carbon": "^1.20|^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartyStudio\\SmartyTerminal\\TerminalServiceProvider"
            ]
        }
    }
}
